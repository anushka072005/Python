# 1) CALCULATING SALARY 
     
working_hourse = list(map(int, input("Enter weekly hourse : ").split(" ")))  
salary_wages = input("Enter wages : ")
sum = 0

for i in working_hourse:
    sum += i
total_salary = (sum * salary_wages)

print(total_salary)

working_hourse = [int(x) for x in input("Enter hourse per day in entire week, separated by spaces : ").split()]
wage = int(input("Enter hourly wase : "))

total = sum(working_hourse)
salary = total * wage
print(salary)
